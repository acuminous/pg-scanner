{
  "extends": ["eslint-config-airbnb-base"],
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2022"
  },
  "rules": {
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "lines-between-class-members": 0,
    "max-classes-per-file": 0,
    "max-len": 0,
    "object-curly-newline": 0,
    "no-await-in-loop": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0
  }
}
